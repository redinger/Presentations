- title 'Editing Available Command'

- form_for(@available_command) do |f|
  = f.error_messages

  %p
    = f.label :name
    %br
    = f.text_field :name
  %p
    = f.label :command
    %br
    = f.text_field :command
  %p
    = f.submit 'Update'
= link_to 'Show', @available_command |
= link_to 'Back', available_commands_path